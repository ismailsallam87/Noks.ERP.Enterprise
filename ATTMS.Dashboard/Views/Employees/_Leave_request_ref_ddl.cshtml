@using ATTMS.Dashboard.Resources 
@using Models
@model List<SP_Select_Leave_request_ref_Result>

<div  class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">@Resource.Leave</label>
        <div class="col-sm-10">
            <select class="form-control m-b"  id="Leave_Ref_id_ddl" name="Leave_Ref_id_ddl">
                <option value="">@Resource.Leave_Reference </option>
                @foreach (var item in Model)
                {
                    <option value="@item.Id"> @item.leave_request_title </option>
                }
            </select>
        </div>
    </div>
</div>

