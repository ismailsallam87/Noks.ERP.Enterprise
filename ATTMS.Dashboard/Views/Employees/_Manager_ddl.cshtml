@using ATTMS.Dashboard.Resources 
@using Models
@model List<SP_select_Employee_Result>

<form method="get" class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label">@Resource.Manager</label>
        <div class="col-sm-10">
            <select class="form-control m-b" id="Manager_id_ddl" name="account">
                <option value="-1">@Resource.Manager_Reference</option>
                @foreach (var item in Model)
                {
                    <option value="@item.Id"> @item.name </option>
                }
            </select>
        </div>
    </div>
</form>

